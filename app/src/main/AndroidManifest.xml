        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cabstwo">
        <activity android:name=".activity.BookingInfo"></activity>
        <activity android:name=".activity.CabList" />
        <activity android:name=".activity.ChangePasswordActivity2" />
        <activity android:name=".activity.Sign_upActivity" />
        <activity android:name=".activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.Content" />
    </application>

</manifest>